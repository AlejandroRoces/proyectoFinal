{
    "name": "camptrack-app",
    "version": "1.0.0",
    "description": "Un proyecto web con HTML, CSS, JS, PHP y MySQL.",
    "main": "index.html",
    "scripts": {
      "start": "live-server --port=3000 --no-browser",
      "dev": "browser-sync start --server --files '**/*.html, **/*.css, **/*.js'",
      "format": "prettier --write '**/*.html' '**/*.css' '**/*.js'"
    },
    "keywords": [
      "web",
      "html",
      "css",
      "js",
      "php",
      "mysql",
      "bootstrap"
    ],
    "author": "Alejandro Roces Fernandez",
    "license": "MIT",
    "dependencies": {
      "bootstrap": "^5.1.3"
    },
    "devDependencies": {
      "live-server": "^1.2.1",
      "browser-sync": "^2.27.10",
      "prettier": "^2.4.1"
    },
    "repository": {
      "type": "git",
      "url": "https://github.com/tu-usuario/camptrack-app.git"
    },
    "bugs": {
      "url": "https://github.com/tu-usuario/camptrack-app/issues"
    },
    "homepage": "https://camptrack.app"
  }
  